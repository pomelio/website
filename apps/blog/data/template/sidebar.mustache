<aside
    class="side-bar-group fixed top-0 left-0 z-20 flex flex-col flex-shrink-0 hidden w-96 h-full pt-16 font-normal duration-75 lg:flex transition-width"
    aria-label="Sidebar">
    <div
        class="relative flex flex-col flex-1 min-h-0 pt-0 bg-white border-r border-gray-200 dark:bg-gray-800 dark:border-gray-700">
        <div class="flex flex-col flex-1 pt-5 pb-4 overflow-y-auto">
            <div class="flex-1 px-3 space-y-1 bg-white divide-y divide-gray-200 dark:bg-gray-800 dark:divide-gray-700">
                <ul class="pb-2 space-y-2">
                    {{#links}}
                    <li>
                        <a href="{{url}}"
                            class="flex {{#active}}active{{/active}} items-center p-2 text-base text-gray-900 transition duration-75 rounded-lg pl-11 group hover:bg-gray-100 dark:text-gray-200 dark:hover:bg-gray-700">{{title}}</a>
                    </li>
                    {{/links}}
                </ul>

            </div>
        </div>

    </div>
</aside>

<div id="side-bar-group-bg" class="side-bar-group fixed inset-0 z-10 hidden bg-gray-900/50 dark:bg-gray-900/90"></div>



<script>  

    $("#toggle-side-bar-mobile").click(e => {
        $(".side-bar-group").toggleClass('hidden');
    });

    $("#side-bar-group-bg").click(e => {
        $(".side-bar-group").toggleClass('hidden');
    });


</script>